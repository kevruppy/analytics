{
    "dbt.enableNewLineagePanel": true,
    "dbt.perspectiveTheme": "Pro Dark",
    "editor.codeActionsOnSave": {
        "source.fixAll.sortJSON": "always"
    },
    "files.associations": {
        "**/dbt_analytics/**/*.sql": "jinja-sql",
        ".yml": "jinja-yaml"
    },
    "python.defaultInterpreterPath": "/workspaces/analytics/.venv/bin/python",
    "python.envFile": "/workspaces/analytics/.env",
    "python.terminal.activateEnvInCurrentTerminal": true,
    "sortJSON.excludedPaths": [
        "/workspaces/analytics/dbt_templates"
    ],
    "sqlfluff.config": "/workspaces/analytics/.sqlfluff",
    "sqlfluff.executablePath": "/workspaces/analytics/.venv/bin/sqlfluff",
    "sqlfluff.linter.run": "onType",
    "yaml.schemas": {
        "https://raw.githubusercontent.com/dbt-labs/dbt-jsonschema/main/schemas/latest/dbt_project-latest.json": [
            "/workspaces/analytics/dbt_analytics/dbt_project.yml"
        ],
        "https://raw.githubusercontent.com/dbt-labs/dbt-jsonschema/main/schemas/latest/dbt_yml_files-latest.json": [
            "/workspaces/analytics/dbt_analytics/*.yml",
            "!profiles.yml",
            "!dbt_project.yml",
            "!packages.yml",
            "!selectors.yml",
            "!profile_template.yml"
        ],
        "https://raw.githubusercontent.com/dbt-labs/dbt-jsonschema/main/schemas/latest/packages-latest.json": [
            "/workspaces/analytics/dbt_analytics/packages.yml"
        ],
        "https://raw.githubusercontent.com/dbt-labs/dbt-jsonschema/main/schemas/latest/selectors-latest.json": [
            "/workspaces/analytics/dbt_analytics/selectors.yml"
        ]
    }
}